{
    "manifest_version": 2,
    "name": "Adblock for Chess.com",
    "description": "Removes ads from Chess.com",
    "version": "1.0.0",
    "icons": {
        "16": "img/icon-16x16.png",
        "32": "img/icon-32x32.png",
        "48": "img/icon-48x48.png",
        "128": "img/icon-128x128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://www.chess.com/*"
            ],
            "css": [
                "styles.css"
            ]
        }
    ]
}